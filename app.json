{
  "expo": {
    "name": "139 Manager",
    "slug": "139-manager",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.139express.pda",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "Мы запрашиваем доступ к вашей камере, чтобы вы могли отсканировать штрих-код посылки в приложении.",
        "NSPhotoLibraryUsageDescription": "Мы запрашиваем доступ к фото-галерее, чтобы вы смогли выбрать изображение документа и загрузить в приложение."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.doolotbekov.pda",
      "versionCode": "1"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "owner": "doolotbekov",
    "extra": {
      "eas": {
        "projectId": "f4ceafac-e44e-4477-8661-98bc39f45fbb"
      }
    },
    "permissions": [ 
      "CAMERA", 
      "AUDIO_RECORDING", 
      "LOCATION", 
      "NOTIFICATIONS", 
      "USER_FACING_NOTIFICATIONS", 
      "SYSTEM_BRIGHTNESS", 
      "USER_LOCATION", 
      "READ_EXTERNAL_STORAGE", 
      "WRITE_EXTERNAL_STORAGE" 
    ]
  }
}
